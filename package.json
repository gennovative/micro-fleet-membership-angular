{
  "name": "micro-fleet-membership-angular",
  "author": "First Interactive Technology",
  "version": "1.0.0",
  "description": "Frontend for project admin app.",
  "scripts": {
    "start": "npm run lint && ng serve --disableHostCheck",
    "start:mobile": "ng serve angular-web-admin-mobile --disableHostCheck",
    "build": "npm run lint && ng build",
    "build:mobile": "npm run lint && ng build angular-web-admin-mobile",
    "build:prod": "npm run lint:app && npm run build -- --prod --aot",
    "test": "lint:app && ng test && test:coverage",
    "e2e": "ng e2e",
    "conventional-changelog": "conventional-changelog",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "lint": "npm run lint:app && npm run lint:styles",
    "lint:app": "ng lint angular-web-admin --fix",
    "lint:e2e": "ng lint angular-web-admin-e2e --fix",
    "lint:styles": "stylelint ./src/**/*.scss --config configs/.stylelintrc.json",
    "ng": "ng",
    "npminstall": "npm install",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "pree2e": "lint:e2e && webdriver-manager update --standalone false --gecko false",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "postinstall": "rimraf node_modules/**/node_modules/typescript"
  },
  "dependencies": {
    "@angular/animations": "~6.1",
    "@angular/common": "~6.1",
    "@angular/compiler": "~6.1",
    "@angular/core": "~6.1",
    "@angular/forms": "~6.1",
    "@angular/http": "~6.1",
    "@angular/platform-browser": "~6.1",
    "@angular/platform-browser-dynamic": "~6.1",
    "@angular/router": "~6.1",
    "@auth0/angular-jwt": "^2.0.0",
    "@fortawesome/fontawesome-free": "~5.2",
    "chart.js": "^2.7.3",
    "classlist.js": "1.1.20150312",
    "core-js": "~2.5",
    "intl": "~1.2",
    "moment": "^2.24.0",
    "normalize.css": "~6.0",
    "pace-js": "~1.0",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "~1.0.0",
    "primeng": "~6.1.3",
    "roboto-fontface": "~0.8",
    "rxjs": "~6.3.3",
    "rxjs-compat": "~6.1",
    "typeface-exo": "~0.0.22",
    "web-animations-js": "~2.2",
    "zone.js": "~0.8"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~2.2.1",
    "@angular-builders/dev-server": "~2.2.2",
    "@angular-devkit/build-angular": "~0.7",
    "@angular/cli": "~6.0",
    "@angular/compiler-cli": "~6.0",
    "@angular/language-service": "~6.0",
    "@compodoc/compodoc": "~1.0",
    "@types/jasmine": "~2.5",
    "@types/jasminewd2": "~2.0",
    "codelyzer": "~4.4",
    "conventional-changelog-cli": "~1.3",
    "husky": "~0.13",
    "jasmine-core": "~2.6",
    "jasmine-spec-reporter": "~4.1",
    "karma": "~1.7",
    "karma-chrome-launcher": "~2.1",
    "karma-cli": "~1.0",
    "karma-coverage-istanbul-reporter": "~1.3",
    "karma-jasmine": "~1.1",
    "karma-jasmine-html-reporter": "~0.2",
    "nodemon": "~1.18",
    "npm-run-all": "~4.0",
    "protractor": "~5.1",
    "rimraf": "~2.6",
    "stylelint": "~7.13",
    "ts-node": "~3.2",
    "tslint": "=5.9.1",
    "typescript": "~2.7"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "repository": "git@github.com:gennovative/micro-fleet-membership-angular.git",
  "private": true
}
