{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "evt-ckin-web-admin": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.png",
              "src/favicon.ico",
              "src/favicon-256x256.ico",
              "src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "../../../node_modules/bootstrap/dist/css/bootstrap.css",
              "../../../node_modules/typeface-exo/index.css",
              "../../../node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "../../../node_modules/@fortawesome/fontawesome-free/scss/fontawesome.scss",
              "../../../node_modules/@fortawesome/fontawesome-free/scss/regular.scss",
              "../../../node_modules/nebular-icons/scss/nebular-icons.scss",
              "../../../node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
              "src/app/theme/styles/styles.scss"
            ],
            "scripts": [
              "../../../node_modules/pace-js/pace.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "evt-ckin-web-admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "evt-ckin-web-admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "evt-ckin-web-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "../../../node_modules/bootstrap/dist/css/bootstrap.css",
              "../../../node_modules/typeface-exo/index.css",
              "../../../node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "../../../node_modules/ionicons/scss/ionicons.scss",
              "../../../node_modules/socicon/css/socicon.css",
              "../../../node_modules/nebular-icons/scss/nebular-icons.scss",
              "../../../node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
              "src/app/theme/styles/styles.scss"
            ],
            "scripts": [
              "../../../node_modules/pace-js/pace.min.js"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/favicon.png"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "typeCheck": true,
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "evt-ckin-web-admin-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "evt-ckin-web-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "evt-ckin-web-admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "evt-ckin-web-admin",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    },
    "@schematics/angular:directive": {}
  }
}